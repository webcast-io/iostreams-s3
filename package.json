{
  "name": "iostreams-s3",
  "version": "0.0.0",
  "description": "File stream provider for iostreams",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node-modules/.bin/grunt",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -- -u exports --report lcovonly -- -R spec -- test/*.js && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:webcast-io/iostreams-s3.git"
  },
  "keywords": [
    "s3",
    "streams",
    "iostreams",
    "provider"
  ],
  "author": "Ben Evans <ben@bensbit.co.uk> (https://bensbit.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webcast-io/iostreams-s3/issues"
  },
  "homepage": "https://github.com/webcast-io/iostreams-s3",
  "devDependencies": {
    "mocha": "~1.15.1",
    "istanbul": "~0.2.4",
    "coveralls": "~2.7.1",
    "grunt": "~0.4.2",
    "grunt-mocha-test": "~0.9.3",
    "grunt-contrib-jshint": "~0.8.0"
  },
  "dependencies": {
    "mkdirp": "~0.3.5",
    "knox": "~0.8.9",
    "underscore": "~1.6.0"
  }
}
